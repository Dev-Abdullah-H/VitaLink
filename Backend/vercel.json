{
    "version": 2,
    "builds": [
      {
        "src": "Backend/app.js",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "16"
        }
      },
      {
        "src": "../FrontEnd/dist",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  